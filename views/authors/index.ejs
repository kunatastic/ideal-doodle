<h2>Search Authors</h2>
<form action="/authors" method="get">
  <label>Name</label>
  <input type="text" name="name" autocomplete="off" value="<%= searchOptions.name %>">
  <button type="submit">Search</button>
</form>

<br>
<% authors.forEach(author=> {%>
  <div>
    <%= author.name %>
  </div>
  <% }); %>